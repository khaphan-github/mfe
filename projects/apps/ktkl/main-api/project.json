{
  "name": "projects-apps-ktkl--main-api",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "projects/apps/ktkl/main-api/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "target": "node",
        "compiler": "tsc",
        "outputPath": "dist/projects/apps/ktkl/main-api",
        "main": "projects/apps/ktkl/main-api/src/main.ts",
        "tsConfig": "projects/apps/ktkl/main-api/tsconfig.app.json",
        "assets": ["projects/apps/ktkl/main-api/src/assets"],
        "webpackConfig": "projects/apps/ktkl/main-api/webpack.config.js"
      },
      "configurations": {
        "development": {},
        "production": {}
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "projects-apps-ktkl--main-api:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "projects-apps-ktkl--main-api:build:development"
        },
        "production": {
          "buildTarget": "projects-apps-ktkl--main-api:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "projects/apps/ktkl/main-api/jest.config.ts"
      }
    }
  },
  "tags": []
}
